<launch>
  <remap from="odom" to="odometry/filtered" />
  <remap from="cmd_vel" to="nav_vel" />

  <node name="move_base"
    pkg="move_base"
    type="move_base"
    clear_params="true">
    <rosparam command="load"
      file="$(find real_leo)/config/move_base/move_base.yaml" />

    <!-- costmaps -->
    <rosparam command="load"
      ns="global_costmap"
      file="$(find real_leo)/config/move_base/costmaps/costmap_common.yaml" />
    <rosparam command="load"
      ns="local_costmap"
      file="$(find real_leo)/config/move_base/costmaps/costmap_common.yaml" />
    <rosparam command="load"
      file="$(find real_leo)/config/move_base/costmaps/local_costmap.yaml" />
    <rosparam command="load"
      file="$(find real_leo)/config/move_base/costmaps/global_costmap.yaml" />

    <!-- planners -->
    <rosparam command="load"
      file="$(find real_leo)/config/move_base/planners/local_planner.yaml" />
    <rosparam command="load"
      file="$(find real_leo)/config/move_base/planners/global_planner.yaml" />
  </node>
</launch>